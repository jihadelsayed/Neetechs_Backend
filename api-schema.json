components:
  schemas:
    AllProfileInfo:
      type: object
      description: |-
        Provides a comprehensive serialization of a User's profile,
        embedding their interests, skills, studies, and experiences directly in the output.
        Also includes category update timestamps and email confirmation status.
      properties:
        id:
          type: integer
          readOnly: true
        stripeCustomerId:
          type: string
          readOnly: true
          nullable: true
        emailConfirmed:
          type: boolean
          nullable: true
          readOnly: true
        subscriptionType:
          type: string
          readOnly: true
          nullable: true
        Interest:
          type: array
          items:
            $ref: '#/components/schemas/Interest'
          readOnly: true
        CompetenceCertificate:
          type: array
          items:
            $ref: '#/components/schemas/CompetenceCertificate'
          readOnly: true
        Study:
          type: array
          items:
            $ref: '#/components/schemas/Study'
          readOnly: true
        Experience:
          type: array
          items:
            $ref: '#/components/schemas/Experience'
          readOnly: true
        CategoryLastupdate:
          type: string
          format: date-time
          nullable: true
          readOnly: true
        SubcategoryLastupdate:
          type: string
          format: date-time
          nullable: true
          readOnly: true
        email:
          type: string
          format: email
          title: Email address
          maxLength: 254
        name:
          type: string
          nullable: true
          maxLength: 50
        first_name:
          type: string
          nullable: true
          maxLength: 50
        phone:
          type: string
          nullable: true
          maxLength: 15
        site_id:
          type: string
          maxLength: 50
        is_creator:
          type: boolean
        bio:
          type: string
          nullable: true
          maxLength: 500
        rating:
          type: string
          format: decimal
          pattern: ^-?\d{0,1}(?:\.\d{0,1})?$
        members:
